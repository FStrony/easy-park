$(function() {
	$("#form\\:exibeTelefones").show();
	RefreshTable();
});

function RefreshTable() {
	if ($("#form\\:exibeTelefones  tr").length <= 1
			|| $("#form\\:exibeTelefones  tr:last td").text() == "") {
		$("#form\\:exibeTelefones").hide();
	} else {
		$("#form\\:exibeTelefones").show();
	}
}