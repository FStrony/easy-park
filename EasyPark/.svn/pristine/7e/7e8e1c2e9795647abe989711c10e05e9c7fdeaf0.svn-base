<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
    "htth://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core">

<ui:composition template="/xhtml/template/_easyparkTemplate.xhtml">
	<ui:define name="extraHeader">
		<h:outputScript library="js" name="maskedinput-1.1.2.pack.js" />

		
	</ui:define>

	<ui:define name="content">
		<div class="boxGray" role="region"
			style="width: 30%; padding-bottom: 10px; padding-left: 100px; padding-top: 10px; padding-right: 100px; margin-left: 25%; margin-top: 3%">
	
		<h:form id="formCadEstac">
			<h:panelGrid columns="2">
				<h:graphicImage value="/resources/img/cadastro.png" width="50"
					height="60" />
				<h1>Alterar senha</h1>
			</h:panelGrid>
			<br />
			<h:panelGrid columns="2">

				<h:outputLabel value="Senha Antiga:" />
				<h:inputSecret id="cnpj" value="#{empresaBean.senhaAntiga}" size="25"
					required="true" requiredMessage="Informe Senha Antiga"
					converter="convertInput" />

				<h:outputLabel value="#{inter.password}:" />
				<h:inputSecret value="#{empresaBean.senha}" size="25"
							required="true" requiredMessage="Informe a senha" />
				
				<h:outputLabel value="Confirme a senha:" />
				<h:inputSecret value="#{empresaBean.confirmeSenha}" size="25"
							required="true" requiredMessage="Informe o confirme senha" />
				
				
				<h:commandButton styleClass="fright btn btn-primary"
					action="#{empresaBean.alterPass}" value="Atualizar" />
				
				

			</h:panelGrid>
			</h:form>
			</div>
			<br />
	</ui:define>
</ui:composition>
</html>