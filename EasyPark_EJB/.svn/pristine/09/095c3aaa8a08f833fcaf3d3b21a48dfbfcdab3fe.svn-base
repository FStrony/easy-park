package br.com.easypark.service.timer;

public class HorarioEstacionamentoVO {

	private String diaUtil;
	private String finalSemana;
	private String feriado;

	public String getDiaUtil() {
		return diaUtil;
	}

	public void setDiaUtil(String diaUtil) {
		this.diaUtil = diaUtil;
	}

	public String getFinalSemana() {
		return finalSemana;
	}

	public void setFinalSemana(String finalSemana) {
		this.finalSemana = finalSemana;
	}

	public String getFeriado() {
		return feriado;
	}

	public void setFeriado(String feriado) {
		this.feriado = feriado;
	}

}
